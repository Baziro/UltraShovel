Addons.Current.Mice = {		ListID = 2,	Name = "",	Description = "",	Author = "EatingPeopleIsFun",	CallType = "Once",	Interval = 0};Addons.Current.Mice.Initialize = func(){	Addons.Current.Mice.Text = Addons.AssignTextFile('prison_mice');	Addons.Current.Mice.Name = Addons.Current.Mice.Text['Name'];	Addons.Current.Mice.Description = Addons.Current.Mice.Text['Description'];}Addons.Current.Mice.Start = func(){	SetPropertyNumber("NPC","PRISON_MOUSE","Scale",10);	SetPropertyNumber("Monster","Monster_Mouse00","Scale",3);	SetPropertyNumber("Item","Ratmeat","Scale",10);	SysMsg(Addons.Current.Mice.Text['Big']);}Addons.Current.Mice.Stop = func(){	SetPropertyNumber("NPC","PRISON_MOUSE","Scale",1);	SetPropertyNumber("Monster","Monster_Mouse00","Scale",1);	SetPropertyNumber("Item","Ratmeat","Scale",1);	SysMsg(Addons.Current.Mice.Text['Small']);}Addons.Current.Mice.MainBody = func(){}Addons.Current.Mice.Initialize();